<html>
	<head>
		<title><%= @title ? @title : "Let's Do Lunch" %></title>
		<script type="text/javascript" src="/javascripts/application.js"     ></script>
		<script type="text/javascript" src="/javascripts/jquery-1.5.js"     ></script>
		<script type="text/javascript" src="/javascripts/standard.js"      ></script>
		<link href="/stylesheets/standard.css" rel="stylesheet" type="text/css" />		
		<script type="text/javascript" src="/javascripts/google_analytics.js"		></script>

	</head>
	<body>
		
		<div class= "mainTop">
			<div class= "main">
				<div class="newEventHeader">
					<h1>Lunchd.com</h1>
				</div>
			</div>
		</div>

		<div class= "mainBody">
			<div class= "dividerBar">
				Let's figure out what were getting for lunch today!
			</div>
		
			<div class= "main">	
				

				<div class= "newEventBody">

					<%= yield %>

					<div class= "clearBoth"></div>
			
				</div>	
			</div>	
		</div>

		<div class= "clearBoth"></div>
		
		<div class= "footer">
			<center>
				<table>
					<tr>
						<td>&copy 2011 lunchd.com</td> 
						<td><img width="115" height="25" vspace="1" src="/images/Powered_By_Yelp_White.png"></td>
					</tr>
				</table>
			</center>
		</div>
	</body>
</html>